{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\index.ts",
      "outFiles": ["${workspaceFolder}/build/**/*.js"],
      "cwd": "${workspaceFolder}",
      "runtimeArgs": ["-r", "./.pnp.js"]
    }
  ]
}
